{
    "name": "artisanry/stringy",
    "description": "Parsedown Wrapper for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-stringy",
        "Laravel Stringy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "danielstjules/stringy": "~3.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Stringy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Stringy\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Stringy\\StringyServiceProvider"
            ],
            "aliases": {
                "Stringy": "Artisanry\\Stringy\\Facades\\Stringy"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
