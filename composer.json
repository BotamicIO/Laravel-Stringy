{
    "name": "faustbrian/laravel-stringy",
    "description": "Parsedown Wrapper for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-stringy", "Laravel Stringy", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "danielstjules/stringy": "~3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Stringy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Stringy\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Stringy\\StringyServiceProvider"
            ],
            "aliases": {
                "Stringy": "BrianFaust\\Stringy\\Facades\\Stringy"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
